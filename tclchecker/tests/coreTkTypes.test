if {[string compare test [info procs resetAnalyzer]] == 1} {
    source [file  join [pwd] [file dirname [info script]] defs.tcl]
}

analyzer::init
renameMessageShow

proc testTypesCmd {cmd} {
    global result
    set result {}
    set ::analyzer::script $cmd
    set ::analyzer::file test
    set ::analyzer::currentLine 1
    analyzer::setQuiet 1
    analyzer::checkScript
    return $result
}

test coreTkTypes-1.0 {coreTk::init} {
    resetAnalyzer
    catch {coreTk::init 4.1} err
    set ::analyzer::checkers(event)
} {{::analyzer::warn warnReserved "Tk 4.2" checkCommand}}


test coreTkTypes-2.0 {coreTk::getMessage} {
    coreTk::getMessage fakeMID
} {fakeMID}
set mid [lindex [array names coreTk::messages] 0]
set str [lindex $coreTk::messages($mid) 0]
test coreTkTypes-2.1 {coreTk::getMessage} {
    coreTk::getMessage $mid
} $str
catch {unset mid str}


test coreTkTypes-3.0 {coreTk::getTypes} {
    coreTk::getTypes fakeMID
} {err}
set mid   [lindex [array names coreTk::messages] 0]
set types [lindex $coreTk::messages($mid) 1]
test coreTkTypes-3.1 {coreTk::getTypes} {
    set mid   [lindex [array names coreTk::messages] 0]
    set types [lindex $coreTk::messages($mid) 1]
    coreTk::getTypes $mid
} $types
catch {unset mid types}

if {0} {

rename coreTk::checkConfigure coreTk::checkConfigureOrig
proc coreTk::checkConfigure {args} {return $args}
test coreTkTypes-4.0 {checkWidgetOptions} {
    checkWidgetOptions 1 {} {a b c} t i
} {1 {a b c} t i}
test coreTkTypes-4.1 {checkWidgetOptions} {
    checkWidgetOptions 1 {-bg} {} t i
} {1 {{-bg coreTk::checkColor}} t i}
    rename coreTk::checkConfigure {}
    rename coreTk::checkConfigureOrig coreTk::checkConfigure

}

